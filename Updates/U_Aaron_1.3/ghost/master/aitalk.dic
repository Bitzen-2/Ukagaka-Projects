//As stated below, the entire minigame was written by Zichqec! I've modfied it slightly, but not too much. Thanks for letting me use the code, Zichqec!
//Written by Zichqec https://zichqec.github.io/s-the-skeleton/
//Main game window
OnJuniorJumble
{
	_word = JumbleWords
	
	//Scramble the word
	_temp = TOLOWER(_word)
	_jumbled = ""
	while STRLEN(_temp) > 0
	{
		_rand = RAND(STRLEN(_temp))
		_jumbled += SUBSTR(_temp,_rand,1)
		_temp = ERASE(_temp,_rand,1)
	}

	"\0\s[6]Sick!\w3\s[29] You get one guess and one guess only to decipher the scrambled word!\n\w5\s[30]You ready to play,\w2 %(username)??\w3\s[22] I sure am!!\n\x\*\![close,inputbox,OnJuniorJumbleGuess,-1]\![open,inputbox,OnJuniorJumbleGuess,-1,--reference=%(_word)]"
	--
	"\s[30]Unscramble this word!!"
	"\s[5]Unsramble this!!"
	"\s[22]Try this on for size!!"
	"\s[22]Bet you won't guess THIS one!!"
	--
	"\n\n\f[height,+4]%(_jumbled)\f[default]"
	--
	"\_q\n\n\![*]\q[I Give Up,OnJuniorJumbleQuit,%(_word)]\e"
}

OnPlayAgain
{
	_word = JumbleWords
	
	_temp = TOLOWER(_word)
	_jumbled = ""
	while STRLEN(_temp) > 0
	{
		_rand = RAND(STRLEN(_temp))
		_jumbled += SUBSTR(_temp,_rand,1)
		_temp = ERASE(_temp,_rand,1)
	}
	
	"\0\s[22]Sick!!\n\w8\*\![close,inputbox,OnJuniorJumbleGuess,-1]\![open,inputbox,OnJuniorJumbleGuess,-1,--reference=%(_word)]"
	--
	"\s[30]Unscramble this word!!"
	"\s[5]Unsramble this!!"
	"\s[22]Try this on for size!!"
	"\s[22]Bet you won't guess THIS one!!"
	--
	"\n\n\f[height,+4]%(_jumbled)\f[default]"
	--
	"\_q\n\n\![*]\q[I Give Up,OnJuniorJumbleQuit,%(_word)]\e"
}

//After the user puts in a guess
OnJuniorJumbleGuess
{
	_word = reference2
	
	if reference0 == ""; "\![raise,OnJuniorJumbleQuit,%(_word)]"
	elseif TOLOWER(reference0) == TOLOWER(_word) //Win
	{
		"\s[30]Yes!!\w3 That's correct!\w6 \s[6]The word was %(_word)!!"
		"\s[30]Well done,\w3 %(username)!!\w4 The word was %(_word)!!\w3\s[6] I'm very proud of you."
	}
	else //Lose
	{
		if reference0 == "idk"
		{
			"\0\s[20]Huh?\w4 Why'd you enter that??\w4 There was a give up box!!\w8\w8\n\n\_q(Seems you've broken Aaron!)\_q\n\w8.\w8.\w8.\w8\w8\n\n\_q(The word was %(_word), by the way)\_q\e"
		}
		//The \_? around the %(reference0) prevents the user from running any sakurascript tags here. Make sure you keep those.
		"\s[6]Nah,\w4 not quite.\w8\w8\s[28] You guessed \_?%(reference0)\_?,\w4 \s[6]whereas the word was %(_word).\w2\s[30] Good guess,\w2 %(username)!"
		"\s[28]Oh?\w3\s[3] Not exactly.\w3\s[6] That's okay.\w5\s[29] The word was %(_word),\w3 by the way!!\n\w6\s[5]Not \_?%(reference0)\_?.\w2 \s[30]But you were close!!"
	}
	--
	Totalminigamesplayed++
	TotalJumbleGamesplayed++
	--
	"\_q\n\n\![*]\q[Let's Play Again!,OnPlayAgain]\n\n\![*]\q[Leave Game,OnOpenMenu]\e"
}

//If the user quits or puts in nothing
OnJuniorJumbleQuit
{
	_word = reference0
	
	"\![close,inputbox,OnJuniorJumbleGuess,-1]"
	--
	"\s[3]Aw,\w4 alright.\w8\w8\s[20] The word was %(_word).\w3\s[6] I bet you could've guessed it if you gave yourself more time!"
	--
	Totalminigamesplayed++
	TotalJumbleGamesplayed++
	--
